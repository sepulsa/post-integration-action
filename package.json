{
  "name": "@sepulsa/post-integration-action",
  "version": "0.0.0",
  "private": true,
  "description": "Post-Integration action",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint . --ext .ts",
    "package:prerelease": "ncc build lib/prerelease.js --out prerelease/dist --source-map --license licenses.txt",
    "package:release": "ncc build lib/release.js --out release/dist --source-map --license licenses.txt",
    "package:route-domain": "ncc build lib/route-domain.js --out route-domain/dist --source-map --license licenses.txt",
    "prepackage": "tsc",
    "package": "npm run package:prerelease && npm run package:release && npm run package:route-domain",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package && npm test",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sepulsa/post-integration-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@aws-sdk/client-route-53": "^3.13.0",
    "patch-package": "^6.4.7",
    "semver": "^7.3.5",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@octokit/request-error": "^2.0.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
